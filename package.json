{
  "name": "trello-mcp-server",
  "version": "1.0.0",
  "description": "A Node.js + TypeScript project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start:local": "node dist/index.js",
    "start:remote": "node dist/src/remote.js",
    "test": "vitest",
    "dev:local": "nodemon src/index.ts",
    "dev:remote": "nodemon src/transports/remote.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "inspect": "npx @modelcontextprotocol/inspector"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "trello",
    "mcp"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.15.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "msw": "^2.11.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.1",
    "@types/express": "^5.0.3",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
